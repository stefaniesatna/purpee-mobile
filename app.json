      "assets/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "purpee.ios",
      "buildNumber": "1.0.0"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#25013D"
      },
      "package": "purpee.android",
      "versionCode": 1
    },
    "web": {
      "favicon": "./assets/icon.png"
    }
  }
}
