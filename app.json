{
  "expo": {
    "name": "Purpee",
    "slug": "stefanie.dev/purpee",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#25013D"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
